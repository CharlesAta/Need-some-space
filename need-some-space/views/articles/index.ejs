<%- include('../partials/header') %>
<h1>Articles</h1>
<% if(articles) { %>
    <% articles.forEach((article) => { %>
    <a href="/articles/<%= article._id %>">
    <div style="background-image: url(<%= article.image %>)">
        <h3><%= article.title %></h3>
    </div>
    </a>
    <% }); %>
<% } else { %>
    <h3>No articles yet</h3>
<% } %>
<%- include('../partials/footer') %>